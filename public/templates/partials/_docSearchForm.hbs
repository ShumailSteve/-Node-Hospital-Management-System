 <form action="{{url}}" method="GET">
                <div class="row filter-row">
                    <div class="col-sm-6 col-md-3 col-lg-3 col-xl-2 col-12">
                        <div class="form-group form-focus">
                            <label class="focus-label">Doctor Name</label>
                            <input type="text" class="form-control floating" name="name">
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-3 col-lg-3 col-xl-2 col-12">
                        <div class="form-group form-focus">
                            <label class="focus-label">Department</label>
                            <input type="text" class="form-control floating" name="department">
                        </div>
                    </div>
                    
                 <div class="col-sm-6 col-md-3 col-lg-3 col-xl-2 col-12">
                        <div class="form-group form-focus">
                            <label class="focus-label">Joined Before</label>
                            <div class="cal-icon">
                                <input class="form-control floating datetimepicker" type="text" name = "joinedBefore">
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-3 col-lg-3 col-xl-2 col-12">
                        <div class="form-group form-focus">
                            <label class="focus-label">Joined After</label>
                            <div class="cal-icon">
                                <input class="form-control floating datetimepicker" type="text" name="joinedAfter">
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-3 col-lg-3 col-xl-2 col-12">
                        <button class="btn btn-success btn-block btn-primary submit-btn"> Search</button>
                       
                    </div>
             </div>
            </form>