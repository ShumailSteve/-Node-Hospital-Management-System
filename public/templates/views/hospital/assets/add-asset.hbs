                    <div class="col-lg-8 offset-lg-2">
                        <form action="hospital/add-asset" method="post">
                            <div class="form-group">
								<label>ID</label>
								<input class="form-control" type="text" name="id">
							</div>
							<div class="form-group">
								<label>Name</label>
								<input class="form-control" type="text" name="name" required>
							</div>
                            <div class="form-group">
								<label>User</label>
								<input class="form-control" type="text" name="user" required>>
							</div>
                            <div class="form-group">
								<label>Purchase Date</label>
								<input class="form-control" type="Date" name="purchaseDate" required>>
							</div>
                              <div class="form-group">
								<label>Warrenty</label>
								<input class="form-control" type="number" name="warrenty" required>
							</div>
                              <div class="form-group">
								<label>Warrenty End</label>
								<input class="form-control" type="Date" name="warrentyEnd" required>>
							</div>
                              <div class="form-group">
								<label>Amount</label>
								<input class="form-control" type="number" name="amount" required>>
							</div>

                              <div class="form-group">
                                <label class="display-block">Status</label>
								<div class="form-check form-check-inline">
									<input class="form-check-input" type="radio" name="status" id="product_active" value="pending" checked>
									<label class="form-check-label" for="product_active">
									Pending
									</label>
								</div>
								<div class="form-check form-check-inline">
									<input class="form-check-input" type="radio" name="status" id="product_inactive" value="approved">
									<label class="form-check-label" for="product_inactive">
									Approved
									</label>
								</div>
                            </div>
                            <div class="m-t-20 text-center">
                                <button class="btn btn-primary submit-btn">Add Asset</button>
                            </div>
                        </form>
